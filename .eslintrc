{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-trailing-spaces": [2, {
            "skipBlankLines": true
        }],
        "no-underscore-dangle": 0
    },
    "globals": {
        "io": true,
        "socket": true
    }
}